   "source": [
    "lst = [1, [2, 3], [4, 5, [6, 7]]]\n",
    "\n",
    "def lst_flatten(lst: list) -> list:\n",
    "    lst1 = []\n",
    "    for x in lst:\n",
    "        if type(x) is int:\n",
    "            lst1.append(x)\n",
    "        else:\n",
    "            lst1.extend(lst_flatten(x))\n",
    "    return lst1\n",
    "\n",
    "print(lst_flatten(lst))"
   ]
